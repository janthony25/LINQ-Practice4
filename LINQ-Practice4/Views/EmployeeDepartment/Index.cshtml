@model List<LINQ_Practice4.Dto.EmployeeDepartmentDto>

<h1>Employee's Department</h1>

<table class="table">
    <thead>
        <tr>
            <th>Employee ID</th>
            <th>Employee Name</th>
            <th>Employee Age</th>
            <th>Department</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <td>@employee.EmployeeId</td>
                <td>@employee.EmployeeName</td>
                <td>@employee.EmployeeAge</td>
                <td>@employee.DepartmentName</td>
            </tr>
        }
    </tbody>

</table>